<div class="new__user ">
	<span class="label__propusk">Пропуск</span>
	<span class="btn__prop">
		<span class="image__propusk icon-left-open"></span>
	</span>

	<form autocomplete="off" id="new-ticket">
		<div class="title-blanc user-data">
			<span id="ticket-number">Пропуск № 54 </span>
			<select name="transaction" id="transaction">
				<option value="out">Отгрузка</option>
				<option value="in">Приход</option>
			</select>
		</div>
		<div class="user-data">
			<div class="label">
				<label for="second-name">Фамилия</label>
				<input autocomplete="off" type="text" id="driver-name" placeholder="Фамилия" />
			</div>
			<div class="label">
				<label for="org">Организация</label>
				<input autocomplete="off" type="text" id="org" placeholder="Организация" />
			</div>
			<div class="label">
				<label for="phone">Паспорт / ВУ</label>
				<input autocomplete="off" type="text" id="document" placeholder="Паспорт / ВУ" />
			</div>
		</div>
		<div class="user-car-data">
			<div class="label">
				<label for="mark-avto">Марка авто</label>
				<input autocomplete="off" type="text" id="mark-avto" placeholder="Марка авто" />
			</div>

			<div class="label">
				<label for="number-avto">Номер авто</label>
				<input autocomplete="off" type="text" id="number-avto" placeholder="Номер авто" />
			</div>
			<div class="label">
				<label for="trailer">Номер прицепа</label>
				<input autocomplete="off" type="text" id="trailer" placeholder="Номер прицепа" />
			</div>
		</div>
		<div class="ticket-result">
			<div id="buttons">
				<div class="reset" id="ticket-clear">Очистить</div>
				<div class="send" id="ticket-send">Отправить</div>
			</div>
		</div>
	</form>
</div>